{
  "manifest_version": 2,
  "name": "meek-browser-extension",
  "minimum_chrome_version": "24",
  "version": "0.1",

  "permissions": [
    "notifications",
    "alarms",
    "management",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
      "scripts": ["background.js"],
      "persistent": true
  }
}
